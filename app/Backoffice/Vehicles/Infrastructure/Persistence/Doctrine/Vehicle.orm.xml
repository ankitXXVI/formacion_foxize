<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="RotateApp\Backoffice\Vehicles\Domain\Vehicle" table="vehicles">
        <id name="id" type="integer" column="id" length="36">
            <generator strategy="AUTO"/>
        </id>
        <field name="nombre" type="string" column="nombre" length="255" />
        <field name="marca" type="string" column="marca" length="255" />
        <field name="modelo" type="string" column="modelo" length="255" />
        <field name="entrada" type="datetime" column="fecha_entrada"/>
        <field name="salida" type="datetime" column="fecha_salida"/>
    </entity>
</doctrine-mapping>